<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="72dp"
    android:orientation="vertical" 
    android:paddingLeft="10dip" >
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="2dp"/>
    <TextView
        android:id="@+id/tv_list_item_ownlib_search_content"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:textSize="20dip" 
        android:text="检索条件：题名=android ">
        
    </TextView>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="4dp"/>
    <TextView
        android:id="@+id/tv_list_item_ownlib_search_time"
        android:layout_width="fill_parent"
        android:layout_height="14dp"
        android:textSize="11dip" 
        android:text="检索时间：2012-10-02 19:28:30">
        
    </TextView>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="2dp"/>
    <TextView
        android:id="@+id/tv_list_item_ownlib_search_url"
        android:layout_width="fill_parent"
        android:layout_height="0dp"/>

</LinearLayout>