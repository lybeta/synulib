<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.skyliuyang.synulib"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
    <!-- 加入访问网络的权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sherlock.Light" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HotBooksActivity" 
            android:label="@string/title_activity_hotbooks">
        </activity>
        <activity
            android:name=".NewBooksActivity"
            android:label="@string/title_activity_newbooks" >
        </activity>
        <activity
            android:name=".HotSearchActivity"
            android:label="@string/title_activity_hotsearch" >
        </activity>
        <activity
            android:name=".OwnLibActivity"
            android:label="@string/title_activity_ownlib" >
        </activity>
        <activity android:name=".CameraTestActivity" >
        </activity>
        <activity android:name=".LoginActivity" 
            android:theme="@style/Theme.Sherlock.Dialog"
            android:label="@string/title_activity_login">
        </activity>
        <activity android:name=".QuickRenewActivity" 
            android:theme="@style/Theme.Sherlock.Dialog"
            android:label="@string/title_activity_quickrenew">
        </activity>
        <activity android:name=".FeedbackActivity" 
            android:theme="@style/Theme.Sherlock.Dialog"
            android:label="@string/title_activity_feeddback">
        </activity>
        <activity android:name=".AboutActivity" 
            android:theme="@style/Theme.Sherlock.Dialog"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>